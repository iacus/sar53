    <div class="grid-col -col2">
        {% for item in presentation %}
            <div class="block">
                <h3 class="block-title text-01">{{ item.title }}</h3>
                {% if item.image %}
                    <img class="block-image" src="{{ item.image }}" alt="{{ item.title }}">
                {% endif %}

                {# {% if item.gallery %}
                    <div class="block-image swiper-container">
                        <div class="swiper-wrapper">
                            {% for image in item.gallery %}
                                <div class="swiper-slide">
                                    <img src="{{ image }}" alt="{{ item.title }}">
                                </div>
                            {% endfor %}
                        </div>
                    </div>
                {% endif %} #}
            </div>
        {% endfor %}
    </div>
</section>

<section class="grid grid-50 section-b">
    <div class="grid-col -col1">
        <p class="title--rotate text-super">
            Tarifas
        </p>
    </div>
    <div class="grid-col -col2">
        {% for item in tarifas[0].blocks %}
                <div class="block">
                    <h3 class="block-title text-01">{{ item.title }}</h3>
                </div>
        {% endfor %}
    </div>
</section>