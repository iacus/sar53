<section class="section">

  <div class="block">

    {# {% if contact.image %}
      {% image "src/"+contact.image , "block-image mb-image img-contain", contact.title, "(min-width: 30em) 50vw, 100vw" %}
    {% endif %} #}

    {% if contact.iframe %}
      <div class="g-col-12 mb-5 block-map embed-responsive embed-responsive-16by9">
        {{contact.iframe|safe}}
      </div>
    {% endif %}

    <h2 class="g-col-12 section-title text-super">Contacto</h2>

    {% if contact.title %}
      <h3 class="g-col-12 block-title- text-01 mb-3">{{ contact.title|safe }}</h3>
    {% endif %}

    {% for item in contact.social %}
      <p class="g-col-12 text-01 mb-5">
        <a href="{{item.url}}" target="_blank">{{item.title}}</a>
      </p>
    {% endfor %}

  </div>
</section>
